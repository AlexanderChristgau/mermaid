{
    "model": {
        "deformation": {
            "compute_similarity_measure_at_low_res": false, 
            "map_low_res_factor": 1.0, 
            "use_map": true
        }, 
        "registration_model": {
            "forward_model": {
                "number_of_time_steps": 10, 
                "smoother": {
                    "multi_gaussian_stds": [
                        0.02, 
                        0.06, 
                        0.06
                    ], 
                    "multi_gaussian_weights": [
                        0.2, 
                        0.3, 
                        0.5
                    ], 
                    "type": "multiGaussian"
                }
            }, 
            "loss": {
                "display_max_displacement": false, 
                "limit_displacement": false, 
                "max_displacement": 0.05
            }, 
            "similarity_measure": {
                "develop_mod_on": false, 
                "sigma": 0.1, 
                "type": "ncc"
            }, 
            "spline_order": 1, 
            "type": "svf_vector_momentum_map"
        }
    }, 
    "optimizer": {
        "name": "sgd", 
        "scheduler": {
            "factor": 0.5, 
            "patience": 10, 
            "verbose": true
        }, 
        "sgd": {
            "individual": {
                "dampening": 0.0, 
                "lr": 1e-05, 
                "momentum": 0.9, 
                "nesterov": true, 
                "weight_decay": 0.0
            }, 
            "shared": {
                "dampening": 0.0, 
                "lr": 1e-05, 
                "momentum": 0.9, 
                "nesterov": true, 
                "weight_decay": 0.0
            }
        }, 
        "single_scale": {
            "nr_of_iterations": 10, 
            "rel_ftol": 1e-08
        }, 
        "use_step_size_scheduler": true
    }
}